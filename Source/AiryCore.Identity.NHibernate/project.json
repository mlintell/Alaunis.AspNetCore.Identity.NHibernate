    "Microsoft.Extensions.DependencyInjection": "1.0.0",
    "NHibernate": "4.0.4.4000"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.ComponentModel.DataAnnotations": "4.0.0.0"
      }
