  "version": "1.0.0-*",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../Tools/Key.snk",
    "xmlDoc": true
  },
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/mlintell/Alaunis.AspNetCore.Identity.NHibernate"
    },
    "tags": [
      "aspnetcore",
      "nhibernate",
      "identity",
      "membership"
